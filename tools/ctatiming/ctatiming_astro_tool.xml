<tool id="ctatiming_astro_tool" name="ctatiming" version="0.0.1+galaxy0" profile="23.0">
  <requirements>
    <requirement type="package" version="8.26.0">ipython</requirement>
    <requirement type="package" version="3.9.1">matplotlib</requirement>
    <requirement type="package" version="6.1.2">astropy</requirement>
    <requirement type="package" version="2.0.1">numpy</requirement>
    <requirement type="package" version="1.2.20">oda-api</requirement>
  </requirements>
  <command detect_errors="exit_code">python '$__tool_directory__/ctatiming.py'</command>
  <configfiles>
    <inputs name="inputs" filename="inputs.json" data_style="paths" />
  </configfiles>
  <inputs>
    <param name="event_file" type="data" label="event_file" format="data" />
    <param name="tbin_s" type="float" value="1000.0" label="tbin_s" />
  </inputs>
  <outputs>
    <data label="${tool.name} -&gt; ctatiming output" name="out_ctatiming_output" format="auto" from_work_dir="output_galaxy.output" />
  </outputs>
  <tests>
    <test expect_num_outputs="1">
      <param name="event_file" location="https://gitlab.renkulab.io/astronomy/mmoda/ctatiming/-/raw/dfaacd7a4e52ed428170c35f0cbe4bf549e9c5ff/events.fits" />
      <param name="tbin_s" value="1000.0" />
      <assert_stdout>
        <has_text text="*** Job finished successfully ***" />
      </assert_stdout>
    </test>
  </tests>
</tool>